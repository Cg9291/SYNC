function getIndexToIns(arr, num) {
  let sorting=(a,b)=>a-b;
  let sortedArr=arr.sort(sorting);
  for(let i=0; i<=sortedArr.length-1;i++){
    if(sortedArr[i]==num && num<sortedArr[i+1]){
      return sortedArr.indexOf(sortedArr[i])
    }else if(sortedArr[i]<num && num<sortedArr[i+1]){
      return sortedArr.indexOf(sortedArr[i+1])
    }else if(sortedArr[sortedArr.length-1]<num){
      return sortedArr.length;
    }else if(sortedArr.length<1){
      console.log("empty arr");
    }
  }

}

