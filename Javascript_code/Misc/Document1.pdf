const obj={
  "Melanie":{
           age:2,
           gender:"girl"}
  ,
  "Archie":{
          age:2,
          gender:"boy"}
  ,
  "Rico":{
          age:5,
          gender:"boy"}
  }
  
const{Melanie,Archie,Rico}=obj;
const{Melanie:{name,age,gender}}=obj;const{Archie:{name:name1,age:age1,gender:gender1}}=obj;const{Rico:{name:name2,age:age2,gender:gender2}}=obj;
function addKids(Name,Age,Gender){
  let key=Name;
  obj[key]={age:Age,
               gender:Gender,
  };
  
  //let i=indexOf(newKid);
  return Gender==="boy"?
     `The new child is a ${Gender} named ${Name},he is a ${Age}.` : `The new child is a ${Gender} named ${Name},she is a ${Age}.`;
}

console.log(addKids("James",7,"boy"))
